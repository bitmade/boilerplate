{
  "name": "bitmade-boilerplate",
  "version": "2.1.0",
  "dependencies": {
    "style-helper": "github:bitmade/style-helper"
  },
  "devDependencies": {
    "bitmade-dev-tools": "github:bitmade/dev-tools",
    "webpack-config": "^3.1.0"
  },
  "scripts": {
    "dev": "./node_modules/.bin/bitmade-dev-server",
    "dev-fs": "./node_modules/.bin/bitmade-dev-server --build-assets",
    "build": "NODE_ENV=production ./node_modules/.bin/bitmade-project-export --compile-assets",
    "export": "NODE_ENV=production ./node_modules/.bin/bitmade-project-export --compile-assets --compile-templates"
  }
}
